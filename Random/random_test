v0x600002a982d0_0 .net "rst", 0 0, v0x600002a98510_0;  1 drivers
E_0x600000d9d6c0 .event posedge, v0x600002a98000_0;
    .scope S_0x12de04a70;
T_0 ;
    %pushi/vec4 85, 0, 7;
    %store/vec4 v0x600002a98090_0, 0, 7;
    %pushi/vec4 54, 0, 7;
    %store/vec4 v0x600002a98120_0, 0, 7;
    %end;
    .thread T_0;
    .scope S_0x12de04a70;
T_1 ;
    %wait E_0x600000d9d6c0;
    %load/vec4 v0x600002a982d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 85, 0, 7;
    %assign/vec4 v0x600002a98090_0, 0;
    %pushi/vec4 54, 0, 7;
    %assign/vec4 v0x600002a98120_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x600002a98090_0;
    %parti/s 6, 0, 2;
    %load/vec4 v0x600002a98090_0;
    %parti/s 1, 6, 4;
    %load/vec4 v0x600002a98090_0;
    %parti/s 1, 5, 4;
    %xor;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x600002a98090_0, 0;
    %load/vec4 v0x600002a98090_0;
    %assign/vec4 v0x600002a981b0_0, 0;
    %load/vec4 v0x600002a98120_0;
    %parti/s 6, 0, 2;
    %load/vec4 v0x600002a98120_0;
    %parti/s 1, 6, 4;
    %load/vec4 v0x600002a98120_0;
    %parti/s 1, 5, 4;
    %xor;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x600002a98120_0, 0;
    %load/vec4 v0x600002a98120_0;
    %assign/vec4 v0x600002a98240_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x12de04900;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600002a98360_0, 0, 1;
T_2.0 ;
    %delay 10000, 0;
    %load/vec4 v0x600002a98360_0;
    %inv;
    %store/vec4 v0x600002a98360_0, 0, 1;
    %jmp T_2.0;
    %end;
