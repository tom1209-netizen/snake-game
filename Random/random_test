    .scope S_0x12de04900;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x600002a98510_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600002a98510_0, 0, 1;
    %delay 1000000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x12de04900;
T_4 ;
